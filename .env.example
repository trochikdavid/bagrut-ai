# Supabase Configuration
# Get these values from your Supabase project dashboard: Settings > API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Azure Speech Services
VITE_AZURE_SPEECH_KEY=your-azure-speech-key
VITE_AZURE_SPEECH_REGION=eastus

# OpenAI Configuration
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_OPENAI_TOPIC_DEVELOPMENT_ASSISTANT_ID=your-topic-development-scoring-assistant-id
VITE_OPENAI_TOPIC_DEVELOPMENT_FEEDBACK_ASSISTANT_ID=your-topic-development-feedback-assistant-id
VITE_OPENAI_VOCABULARY_SCORING_ASSISTANT_ID=your-vocabulary-scoring-assistant-id
VITE_OPENAI_VOCABULARY_FEEDBACK_ASSISTANT_ID=your-vocabulary-feedback-assistant-id
VITE_OPENAI_LANGUAGE_SCORING_ASSISTANT_ID=your-language-scoring-assistant-id
VITE_OPENAI_LANGUAGE_FEEDBACK_ASSISTANT_ID=your-language-feedback-assistant-id

# Fluency (4th criterion) - Uses Azure Pronunciation Assessment metrics
VITE_OPENAI_FLUENCY_SCORING_ASSISTANT_ID=your-fluency-scoring-assistant-id
VITE_OPENAI_FLUENCY_FEEDBACK_ASSISTANT_ID=your-fluency-feedback-assistant-id
